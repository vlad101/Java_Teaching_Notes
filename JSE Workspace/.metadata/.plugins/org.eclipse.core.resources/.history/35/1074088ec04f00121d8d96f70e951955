
public class PrintArrayApp 
{

	public static void main(String[] args) 
	{
		String st = "String";
		printString(st, st.length() -1);
	}
	
	public static void printString(String s, int maxSize)
	{
		if(maxSize == 0)
		{
			System.out.println(s.charAt(maxSize));
			return;
		}
		else
		{
			System.out.print(s.charAt(maxSize));
			printString(s, maxSize - 1);
		}
	}
}
