class Point<T>
{
	private T num;
	
	public Point(T x)
	{
		num = x;
	}
	
	public void setNum(T x)
	{
		num = x;
	}
	
	public T getNum()
	{
		return num;
	}
}

public class PointApp {

	public static void main(String[] args) 
	{
		Point<Integer> myPoint = new Point<Integer>(10);
		Point<String> myPoint1 = new Point<String>("ho");
		Point[] arr = new Point[2];
		arr[0] = new Point<Integer>(10);
		arr[1] = new Point<String>("ho");
		for(int i = 0; i < 2; i++)
			arr[i].getNum();
	}

}
