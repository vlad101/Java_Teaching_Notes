
public class PrintArrayApp 
{

	public static void main(String[] args) 
	{
		String st = "String";
		printString(st, st.length() -1);
	}
	
	public static void printString(String s, int size)
	{
		int index = 0;
		if(index == 0)
		{
			System.out.println(s.charAt(index));
			return;
		}
		else
		{
			System.out.println(s.charAt(index));
			printString(s, size - 1);
		}
	}
}
