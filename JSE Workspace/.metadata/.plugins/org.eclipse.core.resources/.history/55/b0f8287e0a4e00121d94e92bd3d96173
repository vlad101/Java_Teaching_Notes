class Box
{
	private String name;
	
	public Box(String n)
	{
		name = n;
	}
	
	public Box copyObject()
	{
		return new Box(name);
	}
	
	public String getName(){ return name; 
	}
}

public class Test{
	
	public static void main(String[] args){
		
		Box box1, box2;
		box1 = new Box("Square");
		box2 = box1.copyObject();
		if(box1 != box2) System.out.println("Name of box1: " + box1.getName() + "\nName of box1: " + box2.getName());
	}
}

