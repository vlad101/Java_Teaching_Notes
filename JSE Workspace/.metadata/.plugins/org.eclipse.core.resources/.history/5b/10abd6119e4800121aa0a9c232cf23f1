class Student{
	
	private String name;
	private int age;
	
	public Student(String name, int age){
		
		this.name = name;
		this.age = age;
		
	}
}
public class Aggregation 
{
	enum Day {SUN, MON, TUE};
	
	public static void main(String[] args) 
	{
		Day workDay = Day.SUN;
		System.out.println(workDay);
		System.out.println(workDay.toString());
		System.out.println(Day.SUN);


		System.out.println(workDay.ordinal());
		System.out.println(Day.MON.ordinal());
	}

}
