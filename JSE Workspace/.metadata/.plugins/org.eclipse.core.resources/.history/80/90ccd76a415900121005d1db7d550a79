
public class PointApp
{
    public static void main(String[] args)
    {
    	System.out.println("The Greatest Common Divisor of 2 and 3 id " + gcd(49, 28));
    }
    
public static double gcd(double num1, double num2)
{
    if(num1 % num2 == 0)
    	return num2;
    else
    	return gcd(num2, num1 % num2);
}
}