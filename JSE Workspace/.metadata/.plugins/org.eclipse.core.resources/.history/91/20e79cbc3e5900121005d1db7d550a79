
public class PointApp
{
    public static void main(String[] args)
    {
    	System.out.println("The 5th term is 5 * 4 * 3 * 2 * 1 = " + factorial(5));
    }
    
    public static int factorial(int n)
    {
    	if(n == 0)
    		return 1;
    	else
    		return n * factorial(n - 1);
    }
}