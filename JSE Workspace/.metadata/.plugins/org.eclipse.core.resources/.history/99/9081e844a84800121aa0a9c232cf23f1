import java.util.Scanner;

public class Aggregation 
{
	
	
	public static void main(String[] args) 
	{
		Scanner keyboard = new Scanner(System.in);
		System.out.print("Enter a cahracter: ");
		String ans = keyboard.nextLine();
		while(ans = ""){
			char resp = ans.charAt(0);
			if(Character.isDigit(resp))
				System.out.println("Entered didit from 0 through 9");
			if(Character.isLetter(resp))
				System.out.println("Entered ana didit from 0 through 9");
			if(Character.isLetterOrDigit(resp))
				System.out.println("Entered ana didit from 0 through 9");
			if(Character.isLowerCase(resp))
				System.out.println("Entered ana didit from 0 through 9");
			if(Character.isUpperCase(resp))
				System.out.println("Entered ana didit from 0 through 9");
			if(Character.isSpaceChar(resp))
				System.out.println("Entered ana didit from 0 through 9");
			System.out.print("Enter a cahracter: ");
			ans = keyboard.nextLine();
		}
		
	}

}
