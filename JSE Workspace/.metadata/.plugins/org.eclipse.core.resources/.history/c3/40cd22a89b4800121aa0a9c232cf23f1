class Student{
	
	private String name;
	private int age;
	
	public Student(String name, int age){
		
		this.name = name;
		this.age = age;
		
	}
}
public class Aggregation 
{
	enum Day {SUN, MON, TUE};
	
	public static void main(String[] args) 
	{
		Day work1 = Day.SUN;
		System.out.println(work1);
		System.out.println(Day.SUN);
	}

}
