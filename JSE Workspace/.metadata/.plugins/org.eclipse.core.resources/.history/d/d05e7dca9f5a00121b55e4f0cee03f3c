class Node
{
	public int value;
	public Node next;
	
	public Node(int val, Node n)
	{
		value = val;
		next = n;
	}
	
	public Node(int val)
	{
		this(val, null);
	}
}

class SinglyLinkedList
{
	private Node first;
	
	public SinglyLinkedList()
	{
		first = null;
	}
}

public class SinglyLinkedLinkApp 
{

	public static void main(String[] args) 
	{
		
	}

}
