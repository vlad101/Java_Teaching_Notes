
public class Test {

	public static void main(String[] args) {

		for(int j = 0; j <= 5; j++)
			if(j % 2 == 0){
				for(int k = j; k >= 0; k--)
					System.out.print("*");
				System.out.println();
				
			}
		//System.out.println();
		for(int j = 5; j <= 0; j--)
			if(j % 2 == 0){
				for(int k = j; k >= 0; k--)
					System.out.print("*");
				System.out.println();
				
			}
		System.out.println();

	}

}
